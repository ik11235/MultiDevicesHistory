{
  "name": "MultiDevicesHistory-DEV",
  "version": "0.2",
  "manifest_version": 2,
  "description": "__MSG_Description__",
  "default_locale": "en",
  "permissions": ["sessions", "tabs"],
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png"
    }
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistant": false
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
